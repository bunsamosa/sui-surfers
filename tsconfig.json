{
  "extends": "@vue/tsconfig/tsconfig.web.json",
  "include": [
    "env.d.ts",
    "src/**/*",
    "src/**/*.vue"
  ],
  "compilerOptions": {
    "baseUrl": ".",
    "forceConsistentCasingInFileNames": false,
    "strictPropertyInitialization": false,
    "noImplicitAny": false,
    "allowJs": true,
    "paths": {
      "@/*": [
        "./src/*"
      ]
    },
    "esModuleInterop": true
  },
  "references": [
    {
      "path": "./tsconfig.config.json"
    }
  ]
}
